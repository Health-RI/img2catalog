{
  "title": "Health-RI v2 - form version 3",
  "display": "form",
  "description": "",
  "settings": {},
  "components": [
    {
      "label": "General information",
      "type": "content",
      "key": "content",
      "html": "<p>This form is based on the Dataset class of the Health-RI v2 metadata model. For more information, see <a href=\"https://github.com/Health-RI/health-ri-metadata/tree/v2.0.0\" target=\"_blank\">https://github.com/Health-RI/health-ri-metadata/tree/v2.0.0</a>.</p>"
    },
    {
      "type": "panel",
      "title": "Dataset Metadata",
      "components": [
        {
          "key": "identifier",
          "type": "textfield",
          "label": "Identifier",
          "validate": {
            "required": true
          },
          "description": "An unambiguous reference to the resource within a given context. If your data is published in a repository, fill in with the provided DOI. If not, use the identifier of the dataset as generated in the FAIR data point (FDP).",
          "input": true,
          "tableView": true
        },
        {
          "key": "access_rights",
          "data": {
            "values": [
              {
                "label": "Public (Open Data)",
                "value": "http://publications.europa.eu/resource/authority/access-right/PUBLIC"
              },
              {
                "label": "Restricted (Protected Data)",
                "value": "http://publications.europa.eu/resource/authority/access-right/RESTRICTED"
              },
              {
                "label": "Non-public (Sensitive Data)",
                "value": "http://publications.europa.eu/resource/authority/access-right/NON_PUBLIC"
              }
            ]
          },
          "type": "select",
          "label": "Access Rights",
          "validate": {
            "required": true
          },
          "description": "Information about who access the resource or an indication of its security status. Options: public, restricted, non-public.",
          "input": true,
          "tableView": true
        },
        {
          "key": "applicable_legislation",
          "type": "textfield",
          "label": "Applicable Legislation",
          "multiple": true,
          "validate": {
            "required": true
          },
          "description": "The legislation that is applicable to this resource. For health datasets, must include the ELI of the EHDS Regulation (http://data.europa.eu/eli/reg/2025/327/oj).",
          "defaultValue": [
            "http://data.europa.eu/eli/reg/2025/327/oj"
          ],
          "input": true,
          "tableView": true
        },
        {
          "key": "theme",
          "data": {
            "values": [
              "http://publications.europa.eu/resource/authority/data-theme/TECH",
              "http://publications.europa.eu/resource/authority/data-theme/TRAN",
              "http://publications.europa.eu/resource/authority/data-theme/REGI",
              "http://publications.europa.eu/resource/authority/data-theme/SOCI",
              "http://publications.europa.eu/resource/authority/data-theme/AGRI",
              "http://publications.europa.eu/resource/authority/data-theme/ECON",
              "http://publications.europa.eu/resource/authority/data-theme/JUST",
              "http://publications.europa.eu/resource/authority/data-theme/OP_DATPRO",
              "http://publications.europa.eu/resource/authority/data-theme/HEAL",
              "http://publications.europa.eu/resource/authority/data-theme/INTR",
              "http://publications.europa.eu/resource/authority/data-theme/ENVI",
              "http://publications.europa.eu/resource/authority/data-theme/GOVE",
              "http://publications.europa.eu/resource/authority/data-theme/EDUC",
              "http://publications.europa.eu/resource/authority/data-theme/ENER"
            ]
          },
          "type": "select",
          "label": "Theme",
          "multiple": true,
          "validate": {
            "required": true
          },
          "description": "A main category of the resource. A resource can have multiple themes. All health datasets will use the theme 'HEAL', but additional values are allowed.",
          "defaultValue": [
            "http://publications.europa.eu/resource/authority/data-theme/HEAL"
          ],
          "input": true,
          "tableView": true
        }
      ],
      "collapsible": true,
      "input": false,
      "key": "panel",
      "tableView": false,
      "label": "Panel"
    },
    {
      "type": "panel",
      "title": "Contact Point",
      "components": [
        {
          "label": "Information: Contact Point",
          "type": "content",
          "key": "content_contact_point",
          "html": "<p>Relevant contact information for the cataloged resource. This property points to a contact point (department or person) that can answer questions about the dataset. Whenever possible, use a general contact information (for example from a department) instead of contact information of an individual.</p>"
        },
        {
          "key": "contact_point",
          "type": "fieldset",
          "label": "Contact Point",
          "validate": {
            "required": true
          },
          "components": [
            {
              "key": "contact_point.formatted_name",
              "type": "textfield",
              "label": "Formatted Name",
              "validate": {
                "required": true
              },
              "description": "Provide the full name of the contact point, such as the name of a person or department responsible for communication.",
              "input": true,
              "tableView": true
            },
            {
              "key": "contact_point.email",
              "type": "email",
              "label": "Email",
              "prefix": "mailto:",
              "validate": {
                "required": true
              },
              "description": "Email address for communication. Format: mailto:example@domain.com",
              "input": true,
              "tableView": true
            },
            {
              "key": "contact_point.contact_page",
              "type": "url",
              "label": "Contact Page URL",
              "description": "A webpage that either allows to make contact or contains contact information.",
              "input": true,
              "tableView": true
            }
          ],
          "input": false,
          "tableView": false
        }
      ],
      "collapsible": true,
      "input": false,
      "key": "panel",
      "tableView": false,
      "label": "Panel"
    },
    {
      "type": "panel",
      "title": "Creator",
      "components": [
        {
          "label": "Information: Creator",
          "type": "content",
          "key": "content_creator",
          "html": "<p>Creator: This property points to a person (known as Agent) responsible for generating the dataset. In most cases, this should be the projectâ€™s Principal Investigator, provided they consent to being listed in the catalogue. If not, the associated department or institute may be specified instead.</p>"
        },
        {
          "key": "creator",
          "type": "datagrid",
          "label": "Creator",
          "validate": {
            "required": true,
            "minLength": 1
        },
        "components": [
          {
            "type": "tags",
            "key": "name",
            "label": "Name(s)",
            "description": "Name of the entity. Can be repeated for different versions of the name (e.g., in different languages).",
            "validate": {
              "required": true,
              "minLength": 1
            },
            "storeas": "array"
          },
          {
            "type": "tags",
            "key": "identifier",
            "label": "Identifier(s)",
            "description": "ORCID for person, ROR for organization, or other identifiers (ISNI, Wikidata). You can provide multiple identifiers.",
            "validate": {
              "required": true,
              "minLength": 1
            },
            "storeas": "array"
          },
          {
            "key": "mbox",
            "type": "email",
            "label": "Email",
            "prefix": "mailto:",
            "validate": {
              "required": true
            },
            "input": true,
            "tableView": true
          },
          {
            "key": "homepage",
            "type": "url",
            "label": "Homepage",
            "validate": {
              "required": true
            },
            "input": true,
            "tableView": true
          }
        ],
          "input": true,
          "tableView": false
        }
      ],
      "collapsible": true,
      "input": false,
      "key": "panel",
      "tableView": false,
      "label": "Panel"
    },
    {
      "type": "panel",
      "title": "Publisher",
      "components": [
        {
          "key": "publisher",
          "type": "fieldset",
          "label": "Publisher",
          "validate": {
            "required": true
          },
          "components": [
            {
              "label": "Information: Publisher",
              "type": "content",
              "key": "content_Publisher",
              "html": "<p>Publisher: This property identifies the organisation or individual responsible for making the dataset available. For datasets, this is typically the employer of the data creators. In simple cases, the dataset publisher may be the same as the catalog publisher. In more complex settings, such as when datasets come from multiple institutions within a consortium, the consortium should be listed as the publisher where possible. If no formal consortium can be specified, provide the information of the contributing organizations or individuals under dct:creator instead. For more details, refer to the Agent class.</p>"
            },
            {
              "type": "tags",
              "key": "publisher.name",
              "label": "Publisher: Name(s)",
              "description": "Name",
              "validate": {
                "required": true,
                "minLength": 1
              },
              "storeas": "array",
              "input": true,
              "tableView": true
            },
            {
              "key": "publisher.identifier",
              "type": "tags",
              "label": "Publisher: Identifier",
              "validate": {
                "required": true,
                "minLength": 1
              },
              "description": "ROR identifier for organization recommended",
              "storeas": "array",
              "input": true,
              "tableView": true
            },
            {
              "key": "publisher.mbox",
              "type": "email",
              "label": "Publisher: Email",
              "prefix": "mailto:",
              "validate": {
                "required": true
              },
              "input": true,
              "tableView": true
            },
            {
              "key": "publisher.homepage",
              "type": "url",
              "label": "Publisher: Homepage",
              "validate": {
                "required": true
              },
              "input": true,
              "tableView": true
            },
            {
              "key": "publisher.publisher_type",
              "type": "textfield",
              "label": "Publisher: Publisher Type",
              "input": true,
              "tableView": true,
              "placeholder": "Enter publisher type URI (e.g., http://purl.org/adms/publishertype/Company)",
              "validate": {
                "required": true,
                "pattern": "^http://purl\\.org/adms/publishertype/.+",
                "patternMessage": "Value must be from the http://purl.org/adms/publishertype/ domain"
              },
              "tooltip": "Enter a valid URI from the http://purl.org/adms/publishertype/ domain"
            },
            {
              "key": "publisher.publisher_note",
              "type": "textarea",
              "label": "Publisher Note",
              "description": "A description of the publisher activities.",
              "input": true,
              "tableView": true
            }
          ],
          "description": "An entity responsible for making the resource available. Typically the employer of the data creators.",
          "input": false,
          "tableView": false
        }
